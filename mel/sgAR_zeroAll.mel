global proc sgAR_zeroAll (){

string $nodes[] = `ls -l -sl`;
string $attrs[];
string $attr;
string $node;

for($node in $nodes){
	$attrs = `listAttr -s -lf -u -k $node`;
	for($attr in $attrs){
		if((!`gmatch $attr "scale*"`)&&(!`gmatch $attr "visibility"`)&&(!`gmatch $attr "leg*"`)&&(!`gmatch $attr "arm*"`)){
			setAttr ($node + "." + $attr) 0;
			}
		}
	}
	
}
